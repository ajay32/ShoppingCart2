<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hackingbuzz.shoppingcart1" >

    <application
        android:name="com.hackingbuzz.shoppingcart2.application.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name="com.hackingbuzz.shoppingcart2.activites.MainActivity" android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.hackingbuzz.shoppingcart2.activites.CheckoutScreen"/>
        <activity android:name="com.hackingbuzz.shoppingcart2.activites.PaymentScreen"/>
    </application>

</manifest>